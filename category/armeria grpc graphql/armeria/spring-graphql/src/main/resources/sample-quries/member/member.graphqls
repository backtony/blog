query member($id: Long!) {
    member(id: $id) {
        id
        name
        introduction
        type
        country
        team {
            id
            name
            registeredBy
            registeredDate
            modifiedBy
            modifiedDate
        }
        registeredBy
        registeredDate
        modifiedBy
        modifiedDate
    }
}

